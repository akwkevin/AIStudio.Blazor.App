<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:AIStudio.BlazorMaui.Client"
             xmlns:common="clr-namespace:AIStudio.BlazorUI;assembly=AIStudio.BlazorUI"
             x:Class="AIStudio.BlazorMaui.Client.MainPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}">

    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type common:Main}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>