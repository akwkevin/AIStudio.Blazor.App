@namespace AntDesign
@inherits AntDomComponentBase

<div class="@ClassMapper.Class" style="@Style" @ref="Ref" id="@Id">
    <a title="@Title" class="@_titleClass.Class" @onclick="(e)=>OnClick(e)">@Title</a>
    <CascadingValue Value="this" Name="Parent" IsFixed>
        @ChildContent
    </CascadingValue>
</div>