@namespace AntDesign
@inherits AntDomComponentBase

@if (_hasAdd)
{
    <CascadingValue Value="@this.Id" Name="DialogWrapperId">
        <Dialog @ref="_dialog"
                Config="Config"
                Visible="@Visible"
                Style="@Style">
            @ChildContent
        </Dialog>
    </CascadingValue>
}
