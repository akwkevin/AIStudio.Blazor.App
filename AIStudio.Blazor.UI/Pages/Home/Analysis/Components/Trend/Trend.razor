@namespace AIStudio.Blazor.UI.Pages.Home.Analysis
@inherits AntDomComponentBase

<div class="trendItem">
    <span>@ChildContent</span>
    <span class="@Flag">
        @if(Flag == "up"){
            <Icon Type="caret-up" />
        } else {
            <Icon Type="caret-down" />
        }
    </span>
</div>